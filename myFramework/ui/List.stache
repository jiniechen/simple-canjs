<table class={{style}} style="text-align:{{align}} !important">
		<tr class={{titleColor}}>
			{{#each title}}
				{{#each .}}
						<td>{{.}}</td>
				{{/each}}
			{{/each}}
		</tr>
		{{#each data}}
			<tr id="{{@index}}">
				{{#each . }}
					<td>{{.}}</td>
				{{/each}}
			</tr>
		{{/each}}
	</tbody>
</table>

