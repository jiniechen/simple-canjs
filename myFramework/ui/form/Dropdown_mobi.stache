<div class="row has-padding mobi-input {{_align}} divider select no-margin" style="{{_hidden}} !important">
	<span class="cell-4">{{label}}</span>
	{{#if options}}
			<select  {{selectValue}} class="cell-8 no-border" >
				{{#each options}}
					<option value="{{.}}" >{{@key}}</options>
				{{/each}}
			</select>
		{{else}}
			<select  class="cell-12 no-border" >
				<option >Loading text</options>
			</select>
	{{/if}}
</div>

