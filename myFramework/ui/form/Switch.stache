<div class="row has-padding no-margin divider flex space-between relative">
	<label class="cell-6">{{label}}</label>
	<input id="sw" {{switchValue}} class="switch" type="checkbox" name={{name}} value = {{value}}>
	{{#if error.flag}}
		<div class="errorText">
		{{error.message}}
		</div>
	{{/if}}
</div>