{{#each options}}
<label style="justify-content:{{_align}} !important;coloe:#ff4400" class="flex  divider has-padding no-margin" >
	<input  class="checkbox" type="checkbox"  value={{value}} name={{name}}>
	<span>{{label}}</span>
	{{#if error.flag}}
		<div class="help-text text-important has-padding">
		{{error.message}}
		</div>
	{{/if}}
</label>
{{/each}}